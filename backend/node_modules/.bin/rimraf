#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/backend/node_modules/rimraf/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/backend/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/node_modules:/proc/cygdrive/c/Users/abc09/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/backend/node_modules/rimraf/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/backend/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/node_modules:/proc/cygdrive/c/Users/abc09/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/abc09/Desktop/Cohort/Projects/second-brain/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
